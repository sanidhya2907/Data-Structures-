#include <bits/stdc++.h>
using namespace std;
#define int long long int
#define endl "\n"

signed main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n, m;
    cin >> n >> m;
    int i, y;
    vector<int> v, vec;
    for (i = 0; i < n; i++)
    {
        cin >> y;
        v.push_back(y);
    }
    int a, b;
    map<int, string, greater<int>> mp1, mp2;
    map<int, string>::iterator it;
    for (i = 0; i < m; i++)
    {
        cin >> a >> b;
        string s;
        cin >> s;
        auto it = find(v.begin(), v.end(), a);
        if (it != v.end())
        {
            mp1[b] = s;
        }
        else
        {
            mp2[b] = s;
        }
    }
    for (auto &p : mp1)
    {
        cout << p.second << endl;
    }
    for (auto &pr : mp2)
    {
        cout << pr.second << endl;
    }

    return 0;
}
